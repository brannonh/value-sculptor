{
  "name": "value-sculptor",
  "version": "0.2.1",
  "description": "Library for generating pseudo-random dummy values",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npx jest --verbose",
    "test:q": "npx jest",
    "test:w": "npx jest --verbose --watch",
    "test:qw": "npx jest --watch",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint:fix": "npx eslint . --ext .ts,.tsx --fix",
    "prepare": "npm run build",
    "prePublishOnly": "npm run lint:fix && npm run test",
    "preversion": "npm run lint:fix",
    "version": "npm run lint:fix && git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brannonh/value-sculptor.git"
  },
  "keywords": [
    "typescript",
    "value",
    "generate",
    "generator",
    "dummy",
    "random",
    "pseudo-random",
    "demo",
    "data",
    "create"
  ],
  "author": "Brannon Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brannonh/value-sculptor/issues"
  },
  "homepage": "https://github.com/brannonh/value-sculptor#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "1.19.1",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.5"
  }
}
