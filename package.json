{
  "name": "value-sculptor",
  "version": "0.4.0",
  "description": "Library for generating pseudo-random values that follow known patterns and can be used in various ways (e.g., as example data, for testing purposes, etc.).",
  "main": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "npx jest --verbose",
    "test:q": "npx jest",
    "test:w": "npx jest --verbose --watch",
    "test:qw": "npx jest --watch",
    "lint": "npx eslint . --ext .ts,.tsx",
    "lint:fix": "npx eslint . --ext .ts,.tsx --fix",
    "preversion": "npm run lint:fix",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "prePublishOnly": "npm run lint:fix && npm run test",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brannonh/value-sculptor.git"
  },
  "keywords": [
    "typescript",
    "value",
    "generate",
    "generator",
    "dummy",
    "random",
    "pseudo-random",
    "demo",
    "data",
    "create",
    "test",
    "pattern",
    "example",
    "sample",
    "content",
    "utility",
    "spawn",
    "make",
    "propagate",
    "model",
    "substitute",
    "produce",
    "sequence"
  ],
  "author": "Brannon Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brannonh/value-sculptor/issues"
  },
  "homepage": "https://github.com/brannonh/value-sculptor#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.4",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
